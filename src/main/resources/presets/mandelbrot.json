{
	"version": 1,
	"generators": [
		{
			"name": "Mandelbrot",
			"class": "JpegGenerator",
			"path": "mandelbrot.jpeg",
			"imageHandler": {
				"class": "SimpleImageHandler",
				"colorHandler": {
					"class": "BasicColorHandler"
				}
			},
			"calculationHandler": {
				"class": "PoolCalculationHandler",
				"fractal": {
					"class": "MandelbrotSet"
				},
				"plane": {
					"start": {
						"real": -1.0,
						"imaginary": 1.0
					},
					"end": {
						"real": 1.0,
						"imaginary": -1.0
					},
					"step": 0.001
				},
				"maxIterations": 1000,
				"radius": 2
			},
			"width": 4096,
			"height": 4096,
			"offsetX": 0.0,
			"offsetY": 0.0
		}
	]
}