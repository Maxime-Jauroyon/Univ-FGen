{
	"version": 1,
	"generators": [
		{
			"class": "JpegGenerator",
			"path": "fgen-0.jpeg",
			"imageHandler": {
				"class": "SimpleImageHandler",
				"colorHandler": {
					"class": "HsbColorHandler"
				}
			},
			"calculationHandler": {
				"class": "PoolCalculationHandler",
				"parallelismLevel": 16,
				"parallelismThreshold": 16192,
				"fractal": {
					"class": "JuliaSet",
					"complex": {
						"real": 0.3,
						"imaginary": 0.5
					}
				},
				"plane": {
					"start": {
						"real": -1.0,
						"imaginary": 1.0
					},
					"end": {
						"real": 1.0,
						"imaginary": -1.0
					},
					"step": 0.001
				},
				"maxIterations": 100,
				"radius": 2
			},
			"width": 4096,
			"height": 4096,
			"offsetX": 0.0,
			"offsetY": 0.0
		}
	]
}